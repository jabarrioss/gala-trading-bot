{
  "name": "v6",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "echo 'Linting not configured yet'"
  },
  "dependencies": {
    "@gala-chain/gswap-sdk": "^0.0.7",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^17.2.2",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "nodemon": "^3.1.10",
    "sqlite3": "^5.1.7",
    "yahoo-finance2": "^2.13.3"
  },
  "devDependencies": {
    "jest": "^30.1.3",
    "supertest": "^7.1.4"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": ["./tests/setup.js"],
    "collectCoverageFrom": [
      "services/**/*.js",
      "config/**/*.js",
      "utils/**/*.js",
      "!**/node_modules/**"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  }
